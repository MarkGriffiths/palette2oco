{
  "name": "@thebespokepixel/palette2oco",
  "version": "0.1.5",
  "description": "Convert directories of Sip, JSON and ASE palette files to Open Color",
  "main": "index.js",
  "bin": {
    "palette2oco": "./bin/palette2oco.js"
  },
  "scripts": {
    "test": "xo && snyk test && nyc ava",
    "doc-serve": "documentation serve --watch --theme node_modules/documentation-theme-bespoke --github --config src/docs/documentation.yml --name $npm_package_name  --project-version $npm_package_version src/index.js",
    "doc-build": "documentation build --format html --output docs --theme node_modules/documentation-theme-bespoke --github --config src/docs/documentation.yml --name $npm_package_name  --project-version $npm_package_version src/index.js",
    "readme": "compile-readme -u src/docs/example.md src/docs/readme.md > readme.md",
    "code-climate": "nyc report --reporter=text-lcov | codeclimate-test-reporter",
    "coverage": "nyc ava && nyc report --reporter=lcov --report-dir test/coverage; open test/coverage/lcov-report/index.html",
    "clean": "gulp clean",
    "postpublish": "greenkeeper-postpublish"
  },
  "files": [
    "bin/",
    "index-es.js",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarkGriffiths/palette2oco.git"
  },
  "keywords": [
    "Open",
    "Color",
    "oco",
    "sip",
    "ase",
    "json",
    "color",
    "colour",
    "converter"
  ],
  "author": "Mark Griffiths <mark@thebespokepixel.com> (http://thebespokepixel.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MarkGriffiths/palette2oco/issues"
  },
  "homepage": "https://github.com/MarkGriffiths/palette2oco#readme",
  "dependencies": {
    "@thebespokepixel/oco-colorvalue-ex": "^0.2.0",
    "@thebespokepixel/string": "^0.4.3",
    "ase-util": "^1.0.1",
    "common-tags": "^1.3.1",
    "es6-promisify": "^5.0.0",
    "globby": "^6.0.0",
    "lodash": "^4.16.2",
    "opencolor": "^0.2.0",
    "read-pkg": "^2.0.0",
    "shelljs": "^0.7.4",
    "trucolor": "^0.6.0",
    "truwrap": "^0.6.3",
    "update-notifier": "^1.0.2",
    "verbosity": "^0.8.1",
    "yargs": "^6.0.0"
  },
  "devDependencies": {
    "@thebespokepixel/cordial": "^0.17.10",
    "ava": "^0.16.0",
    "codeclimate-test-reporter": "^0.4.0",
    "documentation": "^4.0.0-beta11",
    "documentation-theme-bespoke": "^0.3.1",
    "greenkeeper-postpublish": "^1.0.1",
    "gulp": "github:gulpjs/gulp#4.0",
    "nyc": "^8.3.2",
    "snyk": "^1.19.1",
    "xo": "^0.17.0"
  },
  "xo": {
    "semicolon": false,
    "esnext": true,
    "ignores": [
      "index*.js",
      "bin/**",
      "lib/**",
      "docs/**"
    ]
  },
  "badges": {
    "github": "MarkGriffiths",
    "npm": "thebespokepixel",
    "name": "palette2oco",
    "providers": {
      "status": {
        "text": "beta",
        "color": "blue"
      },
      "aux1": {
        "title": "github",
        "text": "source",
        "color": "4E73B6",
        "link": "https://github.com/MarkGriffiths/palette2oco"
      }
    },
    "readme": {
      "Status": [
        [
          "status",
          "npm",
          "travis",
          "david"
        ],
        [
          "code-climate",
